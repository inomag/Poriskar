<%- include('../layout/header') %> >


    <!-- LIST -->

    <div class="routesList">
        <% routes.forEach(route => {  %>
            <% if(route.id != 0) { %>
                <div class="text-info routesListItem rectangle">


                    <strong> Name: 
                <%= route.name %>
            </strong>
                    <br> ID:
                    <%= route.id %>
                        <br>

                        <div class="btnContainer">
                            <button id="btn-<%= route.latitude %>" class="btnAddress" onclick="getAddress(`<%= route.latitude %>`, `<%= route.longitude %> `)">See Address
                            </button>
                            <p class="address_details" id="<%= route.latitude %>">
                        </div>

                        <% } %>

                </div>

                <% }); %>


    </div>


